{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "WriteLogsToCloudWatch",
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogStream",
        "logs:PutLogEvents",
        "logs:DescribeLogStreams"
      ],
      "Resource": [
        "arn:aws:logs:*:*:log-group:/openclaw/*",
        "arn:aws:logs:*:*:log-group:/openclaw/*:*"
      ]
    }
  ]
}
